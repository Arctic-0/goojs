{
  "const": {
    "id": "const",
    "inputs": [],
    "outputs": [{
      "name": "value",
      "type": "float"
    }],
    "body": "value = /*data.const*/;",
    "defines": {
      "value": {
        "name": "const",
        "type": "float",
        "default": "1.0"
      }
    }
  },
  "add": {
    "id": "add",
    "inputs": [{
      "name": "x",
      "type": "T",
      "generic": true
    }, {
      "name": "y",
      "type": "T",
      "generic": true
    }],
    "outputs": [{
      "name": "sum",
      "type": "T",
      "generic": true
    }],
    "body": "sum = x + y;"
  },
  "sub": {
    "id": "sub",
    "inputs": [{
      "name": "x",
      "type": "T",
      "generic": true
    }, {
      "name": "y",
      "type": "T",
      "generic": true
    }],
    "outputs": [{
      "name": "sum",
      "type": "T",
      "generic": true
    }],
    "body": "sum = x - y;"
  },
  "mul": {
    "id": "mul",
    "inputs": [{
      "name": "x",
      "type": "T",
      "generic": true
    }, {
      "name": "y",
      "type": "T",
      "generic": true
    }],
    "outputs": [{
      "name": "product",
      "type": "T",
      "generic": true
    }],
    "body": "product = x * y;"
  },
  "mulMV": {
    "id": "mulMV",
    "inputs": [{
      "name": "x",
      "type": "T",
      "generic": true
    }, {
      "name": "y",
      "type": "U",
      "generic": true
    }],
    "outputs": [{
      "name": "product",
      "type": "U",
      "generic": true
    }],
    "body": "product = x * y;"
  },
  "dot": {
    "id": "dot",
    "inputs": [{
      "name": "x",
      "type": "T",
      "generic": true
    }, {
      "name": "y",
      "type": "T",
      "generic": true
    }],
    "outputs": [{
      "name": "product",
      "type": "float"
    }],
    "body": "product = dot(x, y) * /*data.multiplier*/;",
    "defines": {
      "multiplier": {
        "name": "multiplier",
        "type": "float",
        "default": "1.0"
      }
    }
  },
  "max": {
    "id": "max",
    "inputs": [{
      "name": "x",
      "type": "T",
      "generic": true
    }, {
      "name": "y",
      "type": "T",
      "generic": true
    }],
    "outputs": [{
      "name": "m",
      "type": "T",
      "generic": true
    }],
    "body": "m = max(x, y);"
  },
  "clamp": {
    "id": "clamp",
    "inputs": [{
      "name": "x",
      "type": "T",
      "generic": true
    }],
    "outputs": [{
      "name": "clamped",
      "type": "T",
      "generic": true
    }],
    "body": "clamped = clamp(x, /*data.min*/, /*data.max*/);",
    "defines": {
      "min": {
        "name": "min",
        "type": "T",
        "default": "0.0"
      },
      "max": {
        "name": "max",
        "type": "T",
        "default": "1.0"
      }
    }
  },
  "sin01": {
    "id": "sin01",
    "inputs": [{
      "name": "x",
      "type": "T",
      "generic": true
    }],
    "outputs": [{
      "name": "s",
      "type": "T",
      "generic": true
    }],
    "body": "s = sin(x * /*data.multiplier*/) * 0.5 + 0.5;",
    "defines": {
      "multiplier": {
        "name": "multiplier",
        "type": "float",
        "default": "1.0"
      }
    }
  },
  "texture2D": {
    "id": "texture2D",
    "inputs": [{
      "name": "sampler",
      "type": "sampler2D"
    }, {
      "name": "coords",
      "type": "vec2"
    }],
    "outputs": [{
      "name": "r",
      "type": "float"
    }, {
      "name": "g",
      "type": "float"
    }, {
      "name": "b",
      "type": "float"
    }, {
      "name": "a",
      "type": "float"
    }],
    "body": "vec4 value = texture2D(sampler, coords);\nr = value.r;\ng = value.g;\nb = value.b;\na = value.a;"
  },
  "out": {
    "id": "out",
    "inputs": [{
      "name": "r",
      "type": "float"
    }, {
      "name": "g",
      "type": "float"
    }, {
      "name": "b",
      "type": "float"
    }],
    "outputs": [],
    "body": "gl_FragColor = vec4(r, g, b, 1.0);"
  },
  "vec2Comp": {
    "id": "vec2-comp",
    "inputs": [{
      "name": "vec",
      "type": "vec2"
    }],
    "outputs": [{
      "name": "x",
      "type": "float"
    }, {
      "name": "y",
      "type": "float"
    }],
    "body": "x = vec.x; y = vec.y;"
  },
  "vec3": {
    "id": "vec3",
    "inputs": [{
      "name": "x",
      "type": "float"
    }, {
      "name": "y",
      "type": "float"
    }, {
      "name": "z",
      "type": "float"
    }],
    "outputs": [{
      "name": "vec",
      "type": "vec3"
    }],
    "body": "vec = vec3(x, y, z);"
  },
  "vec3Comp": {
    "id": "vec3Comp",
    "inputs": [{
      "name": "vec",
      "type": "vec3"
    }],
    "outputs": [{
      "name": "x",
      "type": "float"
    }, {
      "name": "y",
      "type": "float"
    }, {
      "name": "z",
      "type": "float"
    }],
    "body": "x = vec.x; y = vec.y; z = vec.z;"
  },
  "vec43": {
    "id": "vec43",
    "inputs": [{
      "name": "v3",
      "type": "vec3"
    }],
    "outputs": [{
      "name": "v4",
      "type": "vec4"
    }],
    "body": "v4 = vec4(v3, /*data.w*/);",
    "defines": {
      "multiplier": {
        "name": "w",
        "type": "float",
        "default": "1.0"
      }
    }
  },
  "xyz": {
    "id": "xyz",
    "inputs": [{
      "name": "v4",
      "type": "vec4"
    }],
    "outputs": [{
      "name": "v3",
      "type": "vec3"
    }],
    "body": "v3 = v4.xyz;"
  },
  "vec4": {
    "id": "vec4",
    "inputs": [{
      "name": "x",
      "type": "float"
    }, {
      "name": "y",
      "type": "float"
    }, {
      "name": "z",
      "type": "float"
    }, {
      "name": "w",
      "type": "float"
    }],
    "outputs": [{
      "name": "vec",
      "type": "vec4"
    }],
    "body": "vec = vec4(x, y, z, w);"
  },
  "vec4Comp": {
    "id": "vec4-comp",
    "inputs": [{
      "name": "vec",
      "type": "vec4"
    }],
    "outputs": [{
      "name": "x",
      "type": "float"
    }, {
      "name": "y",
      "type": "float"
    }, {
      "name": "z",
      "type": "float"
    }, {
      "name": "w",
      "type": "float"
    }],
    "body": "x = vec.x; y = vec.y; z = vec.z; w = vec.w;"
  },
  "frag-coord": {
    "id": "frag-coord",
    "inputs": [],
    "outputs": [{
      "name": "coord",
      "type": "vec4"
    }],
    "body": "coord = gl_FragCoord;"
  },
  "diffuse": {
    "id": "diffuse",
    "inputs": [{
      "name": "position",
      "type": "vec3"
    }, {
      "name": "normal",
      "type": "vec3"
    }, {
      "name": "color",
      "type": "vec3"
    }],
    "outputs": [{
      "name": "result",
      "type": "vec3"
    }],
    "body": "result = dot(normalize(position), normal) * color;"
  },
  "diffuse2": {
    "inputs": [{
      "name": "position",
      "type": "vec3"
    }, {
      "name": "color",
      "type": "vec3"
    }, {
      "name": "normal",
      "type": "vec3"
    }, {
      "name": "normalMap",
      "type": "sampler2D"
    }, {
      "name": "binormal",
      "type": "vec3"
    }, {
      "name": "tangent",
      "type": "vec3"
    }, {
      "name": "texCoord0",
      "type": "vec2"
    }],
    "outputs": [{
      "name": "result",
      "type": "vec3"
    }],
    "body": [
      "mat3 tangentToWorld = mat3(tangent, binormal, normal);",
      "vec3 tangentNormal = texture2D(normalMap, texCoord0, 0.0).xyz * 2.0 - 1.0;",
      "vec3 worldNormal = (tangentToWorld * tangentNormal);",
      "vec3 N = normalize(worldNormal);",
      "result = dot(normalize(position), N) * color;"
    ]
  },
  "plasma": {
    "id": "plasma",
    "inputs": [{
      "name": "x",
      "type": "float"
    }, {
      "name": "y",
      "type": "float"
    }, {
      "name": "offset",
      "type": "float"
    }],
    "outputs": [{
      "name": "r",
      "type": "float"
    }, {
      "name": "g",
      "type": "float"
    }, {
      "name": "b",
      "type": "float"
    }],
    "body": "float value = sin(x * /*data.s11*/ + y * /*data.s12*/ + time) +\n sin(x * /*data.s21*/ + y * /*data.s22*/ + time) +\n sin(sqrt(pow(x - 250.0, 2.0) + pow(y - 250.0, 2.0)) * 0.1 + time);\nvalue = sin(value * 5.0) * 0.5 + 0.5;\nr = value;\ng = value;\nb = value;",
    "defines": {
      "s11": {
        "name": "s11",
        "default": "0.01"
      },
      "s12": {
        "name": "s12",
        "default": "0.01"
      },
      "s21": {
        "name": "s21",
        "default": "0.03"
      },
      "s22": {
        "name": "s22",
        "default": "0.05"
      }
    }
  }
}