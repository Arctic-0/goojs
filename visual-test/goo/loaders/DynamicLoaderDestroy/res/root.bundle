}, "feadea6082944ad199cdee65968af23d.script": {
	"body": "var setup = function(args, ctx, goo) {\n\tctx.spheres = [];\n\tctx.sphereMeshData = new goo.Sphere(32, 32, 0.6);\n\tctx.sphereMaterial = new goo.Material(goo.ShaderLib.uber);\n\tctx.sphereMaterial.uniforms.materialDiffuse = [0.2, 0.2, 0.2, 1];\n\tctx.sphereMaterial.uniforms.materialSpecular = [0, 0, 0, 1];\n\tconsole.log('material', ctx.sphereMaterial);\n\tfor (var i=0; i<args.numSpheres; i++) {\n\t\tvar x = randomFloat(-3, 3);\n\t\tvar y = randomFloat(-3, 3);\n\t\tvar z = randomFloat(-3, 3);\n\t\tcreateSphere(ctx, [x, y, z]);\n\t}\n};\n\nvar cleanup = function(args, ctx, goo) {\n\tfor (var i=0; i<ctx.spheres.length; i++) {\n\t\tctx.spheres[i].removeFromWorld();\n\t}\n};\n\nvar parameters = [\n\t{key: 'numSpheres', name: '# Spheres', type: 'int', default: 20}\n];\n\nvar createSphere = function(ctx, pos) {\n\tvar sphereEntity = ctx.world.createEntity(\n\t\tctx.sphereMeshData, \n\t\tctx.sphereMaterial, \n\t\t'Sphere '+ctx.spheres.length, \n\t\tpos).addToWorld();\n\tctx.spheres.push(sphereEntity);\n};\n\nfunction randomFloat(min, max) {\n    return Math.random() * (max - min) + min;\n}",
	"name": "Spheres",
	"license": "CC0",
